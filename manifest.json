{
	"manifest_version": 2,
	
	"name": "Timestamp Conversion",
	"description": "This extension converts timestamp to human-readable date format.",
	"version": "1.0",
	
	"browser_action": {
		"default_icon": "assets/icon-19.png",
		"default_popup": "main.html"
	},
	
	"icons":  {
		"48": "assets/icon-48.png",
		"128": "assets/icon-128.png"
	},
	
	"content_scripts": [
		{
			"matches": ["http://wwww.google.com/*"],
			"js": ["assets/script.js"]
		}
	]
}