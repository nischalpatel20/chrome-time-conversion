{
	"manifest_version": 2,
	
	"name": "Timestamp Conversion",
	"description": "This extension converts timestamp to human-readable date format.",
	"version": "1.0",
	
	"browser_action": {
		"default_icon": "assets/icon.png",
		"default_popup": "main.html"
	},
	
	"content_scripts": [
		{
			"matches": ["http://wwww.google.com/*"],
			"js": ["assets/script.js"]
		}
	]
}